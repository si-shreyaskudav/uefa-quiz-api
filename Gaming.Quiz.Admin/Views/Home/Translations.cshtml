
@model Gaming.Quiz.Admin.Models.FeedIngestionModel

<form id="translationsForm"  action="@String.Concat(ViewBag.BasePath, "Translations")" method="post">
    <div class="content-wrapper">
        <div class="container">
            <div class="row pad-botm">
                <div class="col-md-12">
                    <h4 class="header-line">DATA POPULATION</h4>
                </div>
            </div>

            @await Component.InvokeAsync("Message", new { component = ViewBag?.MessageType, message = ViewBag?.MessageText })

            <div class="row">
                <div class="col-md-12">
                    <!-- Advanced Tables -->
                    <div class="panel-body">
                        <div class="table-responsive">
                            <div class="dataTables_wrapper form-inline" role="grid">
                                <table class="table table-striped table-bordered table-hover dataTable no-footer">
                                    <thead>
                                        <tr>
                                            <th class="text-center">Asset</th>
                                            <th class="text-center">Inputs</th>
                                            <th class="text-center">Action</th>
                                        </tr>
                                    </thead>
                                    <tbody>
                                        <tr>
                                            <td class="text-center">Translations</td>
                                            <td class="text-center">
                                                <textarea asp-for="Translations" cols="5" rows="5" style="width:700px;height:300px" class="form-control"></textarea>

                                            </td>
                                            <td class="text-center">
                                                <button type="submit" class="btn btn-info" name="process" value="trans">Ingest</button>
                                            </td>
                                        </tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                    </div>
                    <!--End Advanced Tables -->
                </div>
            </div>
        </div>
    </div>
</form>

